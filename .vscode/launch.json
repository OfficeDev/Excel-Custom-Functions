{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Attach",
            "port": 9229
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}\\src\\customfunctions.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Custom Functions Web Debugging",
            "program": "${workspaceFolder}/node_modules/office-addin-node-debugger/lib/debugger.js",
            "env": {
              "Path": "${workspaceFolder}/node_modules/.bin;${env:Path};"
            },
            "autoAttachChildProcesses": true,
            "sourceMaps": true
        }
    ]
}