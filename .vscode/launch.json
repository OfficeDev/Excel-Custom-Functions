    },
    {
      "name": "Office Online (Chrome)",
      "type": "chrome",
      "request": "launch",
      "webRoot": "${workspaceFolder}",
      "preLaunchTask": "Debug: Web"
    },
    {
      "name": "Office Online (Edge Chromium)",
      "type": "msedge",
      "request": "launch",
      "port": 9222,
