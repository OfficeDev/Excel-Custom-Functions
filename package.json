{
  "name": "excel-functions",
  "version": "0.0.2",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "start": "start npm run dev-server && start npm run packager && npm run sideload",
    "start-web": "start npm run dev-server && start npm run packager",
    "build": "node ./scripts/build.js",
    "buildfast": "node ./scripts/buildfast.js",
    "buildfull": "node ./scripts/buildfull.js",
    "buildmin": "node ./scripts/buildmin.js",
    "bundle-win32": "node ./scripts/build.js native-bundle win32 dev",
    "clean": "node ./scripts/clean.js",
    "packager": "node ./scripts/start-native.js",
    "sideload": "office-toolbox sideload -m manifest.xml -a excel",
    "watch": "node ./scripts/watch.js",
    "enable-debugging": "node ./scripts/enable-debugging.js",
    "disable-debugging": "node ./scripts/disable-debugging.js",
    "tsc": "tsc -p tsconfig.json",
    "start-webpack": "start npm run webpack && start npm run webpack-dev-server && npm run sideload",
    "webpack-dev-server": "webpack-dev-server --mode development --https --key ./certs/server.key --cert ./certs/server.crt --cacert ./certs/ca.crt --port 8081 --hotOnly --inline",
    "webpack": "webpack -p --mode development --watch"
  },
  "dependencies": {
    "chalk": "2.4.1",
    "tslint-microsoft-contrib": "^5.0.1"
  },
  "devDependencies": {
    "@types/es6-collections": "^0.5.29",
    "@types/es6-promise": "0.0.32",
    "@types/flux": "^3.1.0",
    "@types/jquery": "^2.0.39",
    "@types/office-js": "^0.0.37",
    "@types/react": "15.0.38",
    "@types/react-dom": "^15.5.0",
    "@types/react-native": "~0.55.1",
    "file-loader": "^1.1.11",
    "clean-webpack-plugin": "^0.1.19",
    "haul": "^1.0.0-rc.5",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "office-addin-validator": "^1.0.1",
    "office-toolbox": "^0.1.0",
    "react-native": "^0.56.0",
    "react-native-windows": "^0.55.0-rc.0",
    "ts-loader": "^4.4.2",
    "tslint": "^5.9.1",
    "tslint-react": "^3.6.0",
    "typescript": "^2.7.2",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-command": "^0.4.1",
    "webpack-dev-server": "^3.1.1",
    "ws": "^6.0.0"
  },
  "disabledTasks": [
    "copy",
    "dsgen",
    "sass",
    "jest",
    "ts",
    "webpack",
    "tslint"
  ],
  "nativeBundles": [
    {
      "platform": "win32",
      "platformBundleEnabled": true,
      "dev": false
    }
  ]
}
