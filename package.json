{
  "name": "excel-custom-functions",
  "version": "2.0.0",
  "main": "./src/customfunctions.ts",
  "typings": "lib/index.d.ts",
  "scripts": {
    "start": "npm run start-desktop",
    "start-desktop": "office-addin-debugging start manifest.xml --source-bundle-url-path index.win32 --dev-server \"npm run dev-server\" --dev-server-url https://localhost:3000/index.html --packager \"npm run packager\"",
    "start-web": "start npm run dev-server",
    "stop": "office-addin-dev-settings clear manifest.xml",
    "build": "webpack -p --mode production",
    "dev-server": "webpack-dev-server --mode development --https --key ./certs/server.key --cert ./certs/server.crt --cacert ./certs/ca.crt --port 3000 --hotOnly",
    "packager": "haul start --config ./haul.config.js",
    "sideload": "office-toolbox sideload -m manifest.xml -a excel",
    "unload": "office-toolbox remove -m manifest.xml -a excel",
    "watch": "webpack -p --mode development --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/custom-functions-runtime": "^1.4.2",
    "@types/office-runtime": "^1.0.1",
    "babel-preset-react-native": "^4.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "file-loader": "^1.1.11",
    "haul": "^1.0.0-rc.7",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "office-addin-debugging": "^0.0.1",
    "office-addin-dev-settings": "^1.0.0",
    "office-addin-node-debugger": "^0.3.0",
    "office-toolbox": "^0.1.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-native": "^0.57.1",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^4.4.2",
    "typescript": "^2.7.2",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.1"
  }
}
