{
  "name": "excel-custom-functions",
  "version": "2.0.0",
  "main": "./src/customfunctions.js",
  "scripts": {
    "start": "start npm run packager && start npm run dev-server && start npm run watch && npm run sideload",
    "start-web": "start npm run dev-server && start npm run watch",
    "build": "webpack -p --mode production",
    "dev-server": "webpack-dev-server --mode development --https --key ./certs/server.key --cert ./certs/server.crt --cacert ./certs/ca.crt --port 8080 --hotOnly",
    "enable-debugging": "office-addin-dev-settings enable-debugging manifest.xml && office-addin-dev-settings set-source-bundle-url manifest.xml --path index.win32",
    "packager": "haul start --config ./haul.config.js",
    "sideload": "office-toolbox sideload -m manifest.xml -a excel",
    "watch": "webpack -p --mode development --watch"
  },
  "devDependencies": {
    "@types/es6-collections": "^0.5.29",
    "@types/es6-promise": "0.0.32",
    "@types/flux": "^3.1.0",
    "@types/react": "15.0.38",
    "@types/react-dom": "^15.5.0",
    "@types/react-native": "~0.55.1",
    "file-loader": "^1.1.11",
    "haul": "1.0.0-rc.6",
    "html-loader": "^0.5.5",
    "office-addin-dev-settings": "^1.0.0",
    "office-addin-node-debugger": "^0.1.0",
    "office-toolbox": "^0.1.0",
    "react-native": " 0.55.4",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-command": "^0.4.1",
    "webpack-dev-server": "^3.1.1"
  }
}
